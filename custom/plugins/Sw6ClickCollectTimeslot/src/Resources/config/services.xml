<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="Sw6ClickCollectTimeslot\Command\ExampleCommand">
            <tag name="console.command"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\ScheduledTask\ExampleTask">
            <tag name="shopware.scheduled.task"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\Subscriber\MySubscriber">
            <argument type="service" id="request_stack"/>
            <argument type="service" id="order.repository"/>
            <argument type="service" id="Sw6ClickCollectTimeslot\Service\TimeslotService"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\Subscriber\CheckoutConfirmPageSubscriber">
            <argument type="service" id="Sw6ClickCollectTimeslot\Service\TimeslotService"/>
            <argument type="service" id="sales_channel.repository"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\Subscriber\CheckoutValidationSubscriber">
            <argument type="service" id="Sw6ClickCollectTimeslot\Service\TimeslotService"/>
            <argument type="service" id="request_stack"/>
            <tag name="kernel.event_subscriber"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\Storefront\Controller\ExampleController" public="true">
            <call method="setContainer">
                <argument type="service" id="service_container"/>
            </call>
        </service>

        <service id="Sw6ClickCollectTimeslot\Core\Content\Example\SalesChannel\ExampleRoute">
            <argument type="service" id="product.repository"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\Service\CustomFieldsInstaller">
            <argument type="service" id="custom_field_set.repository"/>
            <argument type="service" id="custom_field_set_relation.repository"/>
        </service>

        <service id="Sw6ClickCollectTimeslot\Service\TimeslotService">
            <argument type="service" id="Shopware\Core\System\SystemConfig\SystemConfigService"/>
        </service>

    </services>
</container>